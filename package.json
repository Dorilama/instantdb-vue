{
  "name": "@dorilama/instantdb-vue",
  "version": "0.1.0",
  "description": "Instant DB for Vue",
  "author": "dorilama",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "publish-package": "npm publish --access public",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@instantdb/core": "^0.12.13"
  },
  "peerDependencies": {
    "vue": "^3.0.0-0"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.5.1",
    "mkdist": "^1.5.5",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0"
  },
  "type": "module"
}
