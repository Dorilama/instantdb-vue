{
  "name": "@dorilama/instantdb-vue",
  "version": "0.1.0",
  "description": "Instant DB for Vue",
  "author": "dorilama",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "publish-package": "npm publish --access public"
  },
  "dependencies": {
    "@instantdb/core": "^0.12.13",
    "@types/node": "^22.5.2",
    "@vue/tsconfig": "^0.5.1",
    "mkdist": "^1.5.5",
    "unbuild": "^2.0.0",
    "vite": "^5.4.2"
  },
  "devDependencies": {
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0-0"
  }
}
